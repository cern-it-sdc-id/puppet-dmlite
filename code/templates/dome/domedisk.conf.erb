glb.debug: <%= @disk_debug -%>

glb.role: disk

glb.fcgi.listenport: <%= @disk_port -%>

glb.task.maxrunningtime = <%= @disk_task_maxrunningtime -%>

glb.task.purgetime = <%= @disk_task_purgetime -%>

<% @disk_auth_authorizeDN.sort.each do |entry| -%>
glb.auth.authorizeDN[]: <%= entry %>
<% end -%>

glb.restclient.cli_certificate: <%= @restclient_cli_certificate -%>

glb.restclient.cli_private_key: <%= @restclient_cli_private_key -%>

disk.headnode.domeurl: <%= @headnode_domeurl -%>
