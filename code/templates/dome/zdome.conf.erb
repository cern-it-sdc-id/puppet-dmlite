LoadModule proxy_module modules/mod_proxy.so
LoadModule proxy_fcgi_module modules/mod_proxy_fcgi.so
<%if @dome_head -%>
ProxyPassMatch ^/domehead/(.*)$ fcgi://127.0.0.1:<%= @head_port -%>/$1
<% end -%>
<%if @dome_disk -%>
ProxyPassMatch ^/domedisk/(.*)$ fcgi://127.0.0.1:<%= @disk_port -%>/$1
<% end -%>
ProxyTimeout <%= @proxy_timeout-%> 
